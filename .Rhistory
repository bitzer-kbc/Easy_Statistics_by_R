set.seed(1)
A <- rnorm(50)
B <- rnorm(50)
自尊感情 <- 10*A+50
ソーシャルスキル <- 10*B+50
plot(自尊感情,ソーシャルスキル,xlim=c(0,100), ylim=c(0,100))
points(50,50,cex=45)
legend(50,10,legend="抽出された標本に含まれるデータ",pch=1)
legend(10,100,legend="母集団(円の中全体に広がっていて相関はゼロ")
